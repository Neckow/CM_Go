<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CEM_Go.View.ReferenceDetailPage">
  <ContentPage.Content>
    <StackLayout VerticalOptions="Start" HorizontalOptions="Start" WidthRequest="Infinity" BackgroundColor="Gray">
      <Image Source="{Binding icon}"></Image>
    </StackLayout>
    <Label Text="{Binding project}" VerticalOptions="Center" HorizontalOptions="Center" />
  </ContentPage.Content>
</ContentPage>