<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CEM_Go.View.TeamListPage"
              xmlns:local="clr-namespace:CEM_Go.View;assembly=CEM_Go">
  
  <ContentPage.Content>
    <ListView x:Name="listView" ItemSelected="OnSelection">
      <ListView.ItemTemplate>
        <DataTemplate>
            <ImageCell Text="{Binding name}" Detail="{Binding qualification}" ImageSource="{Binding photo}"/>
        </DataTemplate>
      </ListView.ItemTemplate>
    </ListView>
  </ContentPage.Content>
  
</ContentPage>